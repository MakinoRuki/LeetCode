class Solution {
public:
    bool search(int A[], int n, int target) {
        int l=0,r=n-1;
        while(l<=r){
            int mid=(l+r)/2;
            if(target==A[mid])return true;
            if(A[l]<A[mid]){
                if(target>=A[l] && target<A[mid])r=mid-1;
                else l=mid+1;
            }
            else if(A[l]>A[mid]){
                if(target<A[mid] || target>=A[l])r=mid-1;
                else l=mid+1;
            }
            else l++;
        }
        return false;
    }
};
